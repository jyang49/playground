pipeline {
  agent any 
  options {
    buildDiscarder(logRotator(numToKeepStr: '4'))
    timestamps() 
  }
  stages {
    stage('Build') { 
      steps {
        echo "build 1"
        echo "build 2"
      }
    }
    stage('Test') { 
      steps {
        echo "test 1"
        echo "test 2"
      }
    }
    stage('Deploy') { 
      steps {
        echo "deploy 1"
        echo "deploy 2"
      }
    }
  }
}
